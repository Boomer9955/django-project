pipeline { 
    agent any 
    stages {
        stage('Cloning our Git') { 
            steps { 
                checkout scm
            }
        }
        stage('Building our image') { 
            steps { 
                ansiblePlaybook inventory: 'ansible/hosts.yml', playbook: 'ansible/ansible_script.yml'                  
                    }
                }
            }
        }
